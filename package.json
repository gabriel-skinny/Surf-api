{
  "name": "surf-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:dev": "ts-node-dev 'src/index.ts'",
    "test:functional": "jest --projects ./test --runInBand",
    "test:unit": "jest",
    "test": "yarn test:unit && yarn test:functional",
    "styles:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "styles:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "lint": "eslint ./src ./test --ext .ts"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.0",
    "@types/config": "^0.0.40",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/module-alias": "^2.0.1",
    "@types/nock": "^11.1.0",
    "@types/node": "^13.13.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.5.0",
    "jest": "^27.4.5",
    "nock": "^13.2.1",
    "prettier": "^2.5.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.1",
    "config": "^3.3.6",
    "express": "^4.17.2",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongoose": "^6.1.4"
  }
}
